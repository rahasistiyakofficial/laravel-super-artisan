{
  "name": "rahasistiyak/laravel-super-artisan",
  "description": "Enhanced Artisan tools for Laravel to streamline file generation and workflows",
  "version": "1.0.0",
  "type": "library",
  "keywords": ["laravel", "artisan", "generator", "workflow", "livewire", "vue", "react", "repository"],
  "homepage": "https://github.com/rahasistiyakofficial/laravel-super-artisan",
  "license": "MIT",
  "authors": [
    {
      "name": "Rahas Istiyak",
      "email": "rahasistiyak.official@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "RahasIstiyak\\SuperArtisan\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RahasIstiyak\\SuperArtisan\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RahasIstiyak\\SuperArtisan\\Providers\\SuperArtisanServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "support": {
    "issues": "https://github.com/rahasistiyakofficial/laravel-super-artisan/issues",
    "source": "https://github.com/rahasistiyakofficial/laravel-super-artisan"
  }
}